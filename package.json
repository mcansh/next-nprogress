{
  "name": "@mcansh/next-nprogress",
  "version": "1.1.0-canary.0",
  "description": "Easily integrate NProgress inside your Next.js app using styled-components",
  "repository": "git://github.com/mcansh/next-nprogress.git",
  "author": "Logan McAnsh <logan@mcan.sh> (https://mcan.sh)",
  "source": "src/index.tsx",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "yarn build",
    "dev": "microbundle watch --jsx React.createElement --format cjs,es --external 'styled-components' --compress false",
    "build": "microbundle --jsx React.createElement --format cjs,es --external 'styled-components'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/nprogress": "0.0.29",
    "@types/react": "16.8.19",
    "@types/react-dom": "16.8.4",
    "@types/styled-components": "4.1.15",
    "@typescript-eslint/eslint-plugin": "1.9.0",
    "@typescript-eslint/parser": "1.9.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-kentcdodds": "14.3.2",
    "eslint-config-prettier": "4.3.0",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-react": "7.13.0",
    "eslint-plugin-react-hooks": "1.6.0",
    "husky": ">=1",
    "lint-staged": ">=8",
    "microbundle": "0.11.0",
    "typescript": "3.5.1"
  },
  "dependencies": {
    "next": "8.1.1-canary.40",
    "next-server": "8.1.1-canary.40",
    "nprogress": "0.2.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "styled-components": "4.2.0"
  },
  "peerDependencies": {
    "next": ">=8.0.0",
    "next-server": ">=8.0.0",
    "nprogress": "^0.2.0",
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0",
    "styled-components": ">=4.0.0"
  }
}
